   "source": [
    "import torch\n",
    "\n",
    "# CPUとCUDAの両方にシードを設定\n",
    "torch.manual_seed(42)\n",
    "torch.cuda.manual_seed(42)\n",
    "\n",
    "# 乱数テンソルの生成\n",
    "random_tensor_cpu = torch.randn(3, 2)\n",
    "random_tensor_gpu = torch.randn(3, 2, device='cuda')\n",
    "\n",
    "print(random_tensor_cpu)\n",
    "print(random_tensor_gpu)"
   ]
  }
